<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- ======================================== -->
        <!-- EXPLOITATIONS AGRICOLES DE DÉMONSTRATION -->
        <!-- ======================================== -->
        
        <!-- Exploitation principale de démonstration -->
        <record id="demo_exploitation_soleil" model="exploitation.agri">
            <field name="nom">Ferme du Soleil Levant</field>
            <field name="code_exploitation">FERME_SOLEIL</field>
            <field name="surface_totale">150.5</field>
            <field name="type_exploitation">mixte</field>
            <field name="adresse">Route de la Plaine, 12345</field>
            <field name="region">Occitanie</field>
            <field name="departement">Haute-Garonne</field>
            <field name="coordonnees_gps">43.6047,1.4442</field>
            <field name="date_creation">2020-01-01</field>
            <field name="statut">active</field>
        </record>

        <!-- Exploitation céréalière spécialisée -->
        <record id="demo_exploitation_cereales" model="exploitation.agri">
            <field name="nom">Domaine des Céréales d'Or</field>
            <field name="code_exploitation">CEREALES_OR</field>
            <field name="surface_totale">89.2</field>
            <field name="type_exploitation">cereales</field>
            <field name="adresse">Chemin des Blés, 67890</field>
            <field name="region">Nouvelle-Aquitaine</field>
            <field name="departement">Gironde</field>
            <field name="coordonnees_gps">44.8378,-0.5792</field>
            <field name="date_creation">2018-03-15</field>
            <field name="statut">active</field>
        </record>

        <!-- Exploitation viticole -->
        <record id="demo_exploitation_vigne" model="exploitation.agri">
            <field name="nom">Château de la Vigne</field>
            <field name="code_exploitation">CHATEAU_VIGNE</field>
            <field name="surface_totale">45.8</field>
            <field name="type_exploitation">viticulture</field>
            <field name="adresse">Avenue des Vignes, 11111</field>
            <field name="region">Occitanie</field>
            <field name="departement">Aude</field>
            <field name="coordonnees_gps">43.2135,2.3516</field>
            <field name="date_creation">2019-06-20</field>
            <field name="statut">active</field>
        </record>

        <!-- ======================================== -->
        <!-- MODÈLES IA DE DÉMONSTRATION -->
        <!-- ======================================== -->
        
        <!-- Modèle IA - Prédiction Rendement Blé -->
        <record id="demo_ia_model_1" model="ia.model">
            <field name="name">Modèle IA - Prédiction Rendement Blé</field>
            <field name="description">Modèle de machine learning pour prédire les rendements du blé basé sur les données climatiques et agronomiques</field>
            <field name="type_modele">regression</field>
            <field name="algorithme">random_forest</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
            <field name="precision">87.5</field>
            <field name="rappel">85.2</field>
            <field name="f1_score">86.3</field>
            <field name="state">active</field>
            <field name="date_creation">2024-01-15</field>
            <field name="derniere_mise_a_jour">2024-08-20</field>
        </record>

        <!-- Modèle IA - Détection Stress Hydrique -->
        <record id="demo_ia_model_2" model="ia.model">
            <field name="name">Modèle IA - Détection Stress Hydrique</field>
            <field name="description">Modèle de classification pour détecter le stress hydrique des cultures en temps réel</field>
            <field name="type_modele">classification</field>
            <field name="algorithme">neural_network</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
            <field name="precision">92.1</field>
            <field name="rappel">89.7</field>
            <field name="f1_score">90.9</field>
            <field name="state">trained</field>
            <field name="date_creation">2024-02-10</field>
            <field name="derniere_mise_a_jour">2024-08-22</field>
        </record>

        <!-- Modèle IA - Optimisation Irrigation -->
        <record id="demo_ia_model_3" model="ia.model">
            <field name="name">Modèle IA - Optimisation Irrigation</field>
            <field name="description">Modèle d'optimisation pour la gestion intelligente de l'irrigation</field>
            <field name="type_modele">optimisation</field>
            <field name="algorithme">genetic_algorithm</field>
            <field name="exploitation_id" ref="demo_exploitation_cereales"/>
            <field name="precision">94.2</field>
            <field name="rappel">91.8</field>
            <field name="f1_score">93.0</field>
            <field name="state">active</field>
            <field name="date_creation">2024-03-05</field>
            <field name="derniere_mise_a_jour">2024-08-18</field>
        </record>

        <!-- ======================================== -->
        <!-- PRÉDICTIONS IA DE DÉMONSTRATION -->
        <!-- ======================================== -->
        
        <!-- Prédiction Rendement Blé - Août 2024 -->
        <record id="demo_ia_prediction_1" model="ia.prediction">
            <field name="name">Prédiction Rendement Blé - Août 2024</field>
            <field name="modele_id" ref="demo_ia_model_1"/>
            <field name="type_prediction">rendement</field>
            <field name="description">Prédiction du rendement de blé pour la récolte d'août 2024 basée sur les conditions climatiques actuelles</field>
            <field name="valeur_predite">95.6</field>
            <field name="confiance">87.5</field>
            <field name="etat">validee</field>
            <field name="date_prediction">2024-08-23 10:00:00</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
        </record>

        <!-- Prédiction Stress Hydrique - Septembre 2024 -->
        <record id="demo_ia_prediction_2" model="ia.prediction">
            <field name="name">Prédiction Stress Hydrique - Septembre 2024</field>
            <field name="modele_id" ref="demo_ia_model_2"/>
            <field name="type_prediction">stress</field>
            <field name="description">Prédiction du stress hydrique pour septembre 2024 basée sur les prévisions météo</field>
            <field name="valeur_predite">0.3</field>
            <field name="confiance">92.1</field>
            <field name="etat">terminee</field>
            <field name="date_prediction">2024-08-23 11:00:00</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
        </record>

        <!-- Prédiction Optimisation Irrigation - Semaine 35 -->
        <record id="demo_ia_prediction_3" model="ia.prediction">
            <field name="name">Prédiction Optimisation Irrigation - Semaine 35</field>
            <field name="modele_id" ref="demo_ia_model_3"/>
            <field name="type_prediction">optimisation</field>
            <field name="description">Optimisation du plan d'irrigation pour la semaine 35 de 2024</field>
            <field name="valeur_predite">78.5</field>
            <field name="confiance">94.2</field>
            <field name="etat">en_cours</field>
            <field name="date_prediction">2024-08-23 14:00:00</field>
            <field name="exploitation_id" ref="demo_exploitation_cereales"/>
        </record>

        <!-- ======================================== -->
        <!-- TABLEAUX DE BORD DE DÉMONSTRATION -->
        <!-- ======================================== -->
        
        <!-- Tableau de bord principal -->
        <record id="demo_dashboard_main_1" model="dashboard.main">
            <field name="name">Tableau de Bord Global - Ferme du Soleil Levant</field>
            <field name="type_dashboard">global</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
            <field name="derniere_mise_a_jour">2024-08-23 12:00:00</field>
            <field name="frequence_mise_a_jour">15min</field>
            <field name="statut">actif</field>
        </record>

        <!-- Tableau de bord météo -->
        <record id="demo_dashboard_meteo_1" model="dashboard.meteo">
            <field name="name">Dashboard Météo - Ferme du Soleil Levant</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
            <field name="periode_analyse">7j</field>
            <field name="derniere_mise_a_jour">2024-08-23 12:00:00</field>
            <field name="temperature_actuelle">24.5</field>
            <field name="humidite_actuelle">65.2</field>
            <field name="precipitation_actuelle">0.0</field>
            <field name="vent_actuel">12.3</field>
            <field name="alertes_actives_count">0</field>
            <field name="niveau_alerte_max">vert</field>
            <field name="statut">actif</field>
        </record>

        <!-- Tableau de bord agricole -->
        <record id="demo_dashboard_agricole_1" model="dashboard.agricole">
            <field name="name">Dashboard Agricole - Ferme du Soleil Levant</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
            <field name="periode_analyse">30j</field>
            <field name="derniere_mise_a_jour">2024-08-23 12:00:00</field>
            <field name="superficie_totale">150.5</field>
            <field name="nombre_parcelles">8</field>
            <field name="type_cultures">Blé, Maïs, Tournesol, Orge</field>
            <field name="saison_courante">ete</field>
            <field name="phase_culture">croissance</field>
            <field name="rendement_actuel">95.6</field>
            <field name="rendement_precedent">92.3</field>
            <field name="evolution_rendement">3.6</field>
            <field name="interventions_planifiees">3</field>
            <field name="cout_interventions">1250.00</field>
            <field name="efficacite_irrigation">78.5</field>
            <field name="stress_hydrique">0.2</field>
            <field name="indice_qualite">82.5</field>
            <field name="biodiversite">75.0</field>
            <field name="statut">actif</field>
        </record>

        <!-- ======================================== -->
        <!-- HISTORIQUES IA DE DÉMONSTRATION -->
        <!-- ======================================== -->
        
        <!-- Historique d'entraînement IA -->
        <record id="demo_ia_historique_1" model="ia.historique.entrainement">
            <field name="modele_id" ref="demo_ia_model_1"/>
            <field name="date_entrainement">2024-08-20 14:00:00</field>
            <field name="precision">87.5</field>
            <field name="rappel">85.2</field>
            <field name="f1_score">86.3</field>
            <field name="erreur_moyenne">0.124</field>
            <field name="duree_entrainement">45.2</field>
            <field name="commentaires">Entraînement réussi avec 1000 échantillons de données agricoles</field>
            <field name="statut">termine</field>
        </record>

        <record id="demo_ia_historique_2" model="ia.historique.entrainement">
            <field name="modele_id" ref="demo_ia_model_2"/>
            <field name="date_entrainement">2024-08-22 09:00:00</field>
            <field name="precision">92.1</field>
            <field name="rappel">89.7</field>
            <field name="f1_score">90.9</field>
            <field name="erreur_moyenne">0.089</field>
            <field name="duree_entrainement">78.5</field>
            <field name="commentaires">Entraînement avec validation croisée 5-fold et augmentation des données</field>
            <field name="statut">termine</field>
        </record>

        <!-- Historique d'exécution IA -->
        <record id="demo_ia_execution_1" model="ia.historique.execution">
            <field name="prediction_id" ref="demo_ia_prediction_1"/>
            <field name="date_execution">2024-08-23 10:00:00</field>
            <field name="etat">terminee</field>
            <field name="duree_execution">12.3</field>
            <field name="ressources_utilisees">CPU: 12%, RAM: 256MB</field>
            <field name="commentaires">Prédiction exécutée avec succès, modèle stable</field>
        </record>

        <record id="demo_ia_execution_2" model="ia.historique.execution">
            <field name="prediction_id" ref="demo_ia_prediction_2"/>
            <field name="date_execution">2024-08-23 11:00:00</field>
            <field name="etat">terminee</field>
            <field name="duree_execution">8.7</field>
            <field name="ressources_utilisees">CPU: 8%, RAM: 128MB</field>
            <field name="commentaires">Prédiction exécutée avec succès, performance optimale</field>
        </record>

        <!-- ======================================== -->
        <!-- RAPPORTS DE TABLEAU DE BORD -->
        <!-- ======================================== -->
        
        <!-- Rapport de tableau de bord -->
        <record id="demo_dashboard_rapport_1" model="dashboard.rapport">
            <field name="dashboard_id" ref="demo_dashboard_meteo_1"/>
            <field name="type_rapport">quotidien</field>
            <field name="date_rapport">2024-08-23 06:00:00</field>
            <field name="statut">termine</field>
            <field name="taille_fichier">1.2 MB</field>
            <field name="actions">Rapport généré et envoyé automatiquement par email</field>
        </record>

        <record id="demo_dashboard_rapport_2" model="dashboard.rapport">
            <field name="dashboard_id" ref="demo_dashboard_agricole_1"/>
            <field name="type_rapport">hebdomadaire</field>
            <field name="date_rapport">2024-08-19 08:00:00</field>
            <field name="statut">termine</field>
            <field name="taille_fichier">3.8 MB</field>
            <field name="actions">Rapport hebdomadaire avec analyse des tendances et recommandations</field>
        </record>
    </data>
</odoo> 