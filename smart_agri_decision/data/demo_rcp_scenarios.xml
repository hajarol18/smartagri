        <!-- Scénario RCP 2.6 - Optimiste -->
        <record id="demo_rcp_26" model="rcp.scenario">
            <field name="name">RCP 2.6 - Optimiste</field>
            <field name="description">Scénario de stabilisation avec émissions très faibles. Stabilisation de la température à +1°C d'ici 2100. Ce scénario suppose une action climatique ambitieuse et immédiate.</field>
            <field name="rcp_type">rcp_26</field>
            <field name="exploitation_id" ref="demo_exploitation_soleil"/>
            <field name="annee_debut">2020</field>
            <field name="annee_fin">2100</field>
            <field name="temperature_rise">1.0</field>
            <field name="co2_concentration">421</field>
            <field name="precipitation_change">-5</field>
            <field name="stress_hydrique_rcp">0.1</field>
            <field name="stress_thermique_rcp">0.05</field>
            <field name="rendement_change">-2.5</field>
            <field name="state">active</field>
        </record>

